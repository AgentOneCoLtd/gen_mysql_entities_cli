{
  "name": "@ag1/gen_mysql_entities_cli",
  "version": "1.1.0",
  "description": "generate most used mysql entity functions in our codebase",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "agent one co., ltd.",
  "license": "MIT",
  "private": false,
  "bin": {
    "gen-mysql-entities": "./lib/index.js"
  },
  "scripts": {
    "start": "node ./lib/index.js",
    "prettier:ts": "prettier --write \"./src/**/*.ts\"",
    "prettier:config": "prettier --no-config --write \"./*.{json,yml}\"",
    "prettier": "yarn prettier:ts --loglevel warn && yarn prettier:config --loglevel warn",
    "lint": "eslint --ext .ts ./src --fix",
    "tsc:spects": "tsc -b tsconfig-spects.json",
    "tsc:onlyts": "tsc -b tsconfig-onlyts.json",
    "compile": "yarn tsc:onlyts && yarn tsc:spects",
    "test": "jest --config \"./jest.config.json\" --passWithNoTests",
    "build": "yarn prettier && yarn lint && yarn compile && yarn test"
  },
  "dependencies": {
    "@ag1/mysql_rx_wrapper": "^1.0.0",
    "@ag1/nil": "^1.0.0",
    "@ag1/return_switch": "^1.0.1",
    "change-case": "^3.1.0",
    "ejs": "^2.6.1",
    "mkdirp": "^0.5.1",
    "mysql": "^2.17.1",
    "prettier": "^1.18.2",
    "rxjs": "^6.5.2",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/jest": "^24.0.14",
    "@types/joi": "^14.3.3",
    "@types/mkdirp": "^0.5.2",
    "@types/mysql": "^2.15.6",
    "@types/node": "^10",
    "@types/prettier": "^1.16.4",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "jest": "^24.8.0",
    "joi": "^14.3.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "keywords": [
    "generate",
    "mysql",
    "entity",
    "mysql_rx_wrapper"
  ],
  "repository": {
    "url": "https://github.com/AgentOneCoLtd/gen_mysql_entities_cli.git",
    "type": "git"
