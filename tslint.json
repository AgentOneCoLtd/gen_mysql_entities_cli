{
  "extends": ["tslint-microsoft-contrib"],
  "rules": {
    "export-name": false,
    "newline-per-chained-call": false,
    "no-relative-imports": false,
    "no-submodule-imports": [true, "rxjs"],
    "no-unnecessary-local-variable": false,
    "quotemark": [true, "single", "avoid-escape"],
    "trailing-comma": true,
    "typedef": false
  }
}
